/* pages/dynamicpage/dynamicpage.wxss */
page{
    --card-background-color:#fff
}
.liststyle {
    padding: 24rpx;
    width: 100%;
    box-sizing: border-box;
    /* display: flex;
  justify-content: space-between;
  align-items: flex-start; */
    flex-wrap: wrap;
}

.ismore {
    font-size: 24rpx;
    color: #ccc;
    padding: 20rpx;
    text-align: center;
}

.card-title {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.card-desc{
}

.card-address {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.footer {
    padding-left: 194rpx;
}

.createTime {
    color: #787878;
    font-size: 22rpx;
    margin-right: 8rpx;
}
.vancard{
    margin-bottom: 27rpx;
    box-shadow: 0 0 20rpx #b1b0b0;
    border-radius: 14rpx;
}
.card-tag{
    position: absolute;
    top: 20rpx;
    left: -20rpx;
}