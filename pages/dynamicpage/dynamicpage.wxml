<!-- 我的动态 -->

<block wx:if="{{list.length>0}}">
    <view class="liststyle">
        <block wx:for="{{list}}" wx:key="index">
            <!-- <DongtaiModel item="{{item}}" isdele="{{isdele}}" bind:ondele="ondele" /> -->
            <block>
                <van-card tag="{{item.identifyStatus}}" desc="{{item.introduction}}" title="{{item.title||'投喂点'}}" thumb="{{item.feedPointMedias[0].url||'/images/catimg.png'}}" thumb-link="{{'/pages/catdetail/catdetail?cat_id='+item.id}}">
                    <view slot="price-top">
                        {{item.address}}
                    </view>
                    <view slot="price">
                        {{item.createTime}}
                    </view>
                    <view slot="footer">
                        <van-button size="mini" data-id="{{item.id}}" bind:click="editcat">编辑</van-button>
                        <van-button size="mini" type="warning" style="margin-left: 10rpx;" data-id="{{item.id}}" bind:click="catDelete">删除</van-button>
                    </view>
                </van-card>
            </block>
        </block>
    </view>
    <view class="ismore" hidden="{{!ismore}}">暂无更多</view>
</block>
<block wx:else>
    <van-empty description="暂无发布" />
</block>